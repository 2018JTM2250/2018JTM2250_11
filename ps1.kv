#:import FadeTransition kivy.uix.screenmanager.FadeTransition
   
ScreenManagement:
	transition: FadeTransition()
	SelectionPage:
	LoginPage:
    StudentPage:
    AdminPage:
    StudentDetailsPage:
    DetailsPage:
   
    



<SelectionPage>:
	name : "selection_page"

    canvas:
    	Rectangle:
    		source: 'iitdelhi.jpg'
            size: self.size
            pos:self.pos
            
            
        Rectangle:
        	source: 'logo.png'
            size:300,300
            pos: (100,700)
            
            
	FloatLayout:
		Label:
			text: "Indian Institute of Technology "
			font_size:70
			color : 0.5,0.5,0.8,1
			pos_hint: {"x": 0, "y": 0.4}
			
			
		Label:
			text: "Delhi"
			font_size:70
			color : 0.5,0.5,0.8,1
			pos_hint: {"x": 0, "y": 0.3}
			
		Label:
			text: " I am "
			font_size:20
			color : 0,0,0,1
			pos_hint: {"x": 0, "y": 0.17}
			
		Label:
			text: " or "
			font_size:20
			color : 0,0,0,1
			pos_hint: {"x": 0, "y": 0.02}
		Button:
			text:"Student"
			font_size:40
			pos_hint : {"x" : 0.450 , "y" : 0.55}
			size_hint : 0.1,0.1
			on_release: root.login_as("student")
			background_color: 1, 0.78, 0.90, 1 
          
          
		Button:
			text:"Admin"
			font_size:40
			pos_hint : {"x" : 0.450 , "y" : 0.40}
			size_hint : 0.1,0.1
			on_release : root.login_as("admin")
			background_color: 1, 0.78, 0.90, 1 	
          
          
          
          
          
<LoginPage>:
    name: "login_page"
    login_statusr : login
    
    canvas:
    	Rectangle:
    		source: 'wallpaper5.png'
            size: self.size
            pos:self.pos
            
        Rectangle:
        	source: 'Logo.png'
            size:200,200
            pos: (850,750)
            
    
    GridLayout:
    	rows:4
    	spacing:20
		FloatLayout:
    		Label:
    			font_size:70
    			text:"Enter Credentials"
    			color : 0,0.9,0.72,1
    			pos_hint: {"x": 0.01, "y": -0.6}
    		
    	
 
		FloatLayout:
			
			Label:
				text: "Enter Username"
				color : 1,1,0.30,1
				font_size:28
				pos_hint: {"x": 0.14, "y": 0.27}
				size_hint : 0.4,0.3
				
        	TextInput:
        		id: login
				pos_hint: {"x": 0.42, "y": 0.34}
				size_hint : 0.3,0.14
				 
				multiline: False  
				font_size:26
				hint_text : "Username"  
				write_tab: False
    		 
            	
		FloatLayout:
			Label:
				text: "Enter Password"
				color : 1,1,0.30,1
				font_size:28
				pos_hint: {"x": 0.14, "y": 0.96}
				size_hint : 0.4,0.3
				
        	TextInput:
        		id: passw
            	password: True # hide password
				pos_hint: {"x": 0.42, "y": 1.03}
				size_hint : 0.3,0.14
				multiline: False
				font_size:26
            	hint_text : "Password"
            	write_tab: False
    		
            	

				
				
			Button:
				pos_hint: {"x": 0.9, "y": 2.9}
				size_hint : 0.10,0.15
    			text: "Home Page"
    			font_size:20
    			background_color: 1, 0, 0, 0.6
				on_release: root.deleting_data()
		          	


			Button:
				pos_hint: {"x": .44, "y": 0.6}
				size_hint : 0.10,0.20
    			text: "Login"
    			font_size:20
    			background_color: 1, 0, 0, 0.6
            	on_release: root.verify_credentials()










